%{
#include "y.tab.h"  // Include the header generated by Yacc (contains token definitions)
%}

%%

[0-9]+      { yylval = atoi(yytext); return NUMBER; }
[+\-*/]     { return yytext[0]; }
[()]        { return yytext[0]; }
[ \t\n]     { /* ignore whitespace */ }
.           { return yytext[0]; }

%%

int yywrap(void) {
    return 1;
}
